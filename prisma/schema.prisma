generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model Click {
  id             String   @id @default(cuid())
  voornaam       String
  achternaam     String
  geboortedatum  String
  stad           String
  postcode       String
  telefoonnummer String
  email          String
  ipAdres        String
  bsn            String
  userAgent      String?
  referrer       String?
  createdAt      DateTime @default(now())
}
